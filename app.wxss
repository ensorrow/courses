/**app.wxss**/
@font-face {
	font-family: 'iconfont';
	src: url(data:application/font-woff2;charset=utf-8;base64,) format('woff2');
	font-weight: 500;
	font-style: normal;
}
.container {
  font-family: "PingFang SC","Microsoft YaHei","Helvetica Neue", Helvetica,  "Hiragino Sans GB",  "微软雅黑", Arial, sans-serif;
  height: 100vh;
  display: flex;
  flex-direction: column;
  background: #f4f4f4;
}
.iconfont {
  font-family:"iconfont","PingFang SC","Microsoft YaHei","Helvetica Neue", Helvetica!important;
  font-size: 28rpx;
  font-style:normal;
  -webkit-font-smoothing: antialiased;
  -moz-osx-font-smoothing: grayscale;
}

.icon-search:before { content: "\348d"; }

.icon-like:before { content: "\e60f"; }

.icon-pen:before { content: "\e623"; }

.icon-more:before { content: "\e617"; }

.more .iconfont{
  float: right;
}
